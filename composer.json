{
    "name": "nitsan/ns-disqus-comments",
    "type": "typo3-cms-extension",
    "description": "Easily configure Disqus.com plugin to your TYPO3 pages, news, blogs etc., Disqus is world one of the most popular comment system. Live-Demo: https://demo.t3terminal.com/t3t-extensions/disqus-comment/ You can download PRO version for more-features & free-support at ttps://t3terminal.com/typo3-disqus-comment-plugin-free/",
    "homepage": "https://extensions.typo3.org/extension/ns_disqus_comments/",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "Disqus",
        "Nitsan",
        "Comments"
    ],
    "support": {
        "issues": "https://github.com/nitsan-technologies/ns_disqus_comments/issues"
    },
    "authors": [],
    "require": {
        "typo3/cms-core": ">=6.2.0"
    },
    "autoload": {
        "psr-4": {
            "Ns\\NsDisqusComments\\": "Classes/"
        }
    },
    "replace": {
        "typo3-ter/ns-disqus-comments": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_disqus_comments"
        }
    }
}
